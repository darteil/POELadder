{
  "name": "poe-ladder",
  "version": "1.0.5",
  "description": "",
  "main": "electron/main.js",
  "browserslist": [
    "last 1 version"
  ],
  "scripts": {
    "start": "./node_modules/.bin/electron .",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./config/webpack.dev.js --mode development",
    "prod": "./node_modules/.bin/webpack --config ./config/webpack.prod.js --mode production",
    "eslint": "./node_modules/.bin/eslint --fix ./src",
    "prettier": "./node_modules/.bin/prettier --config ./.prettierrc.js ./src/**/*.jsx ./src/**/*.js --write",
    "pack": "./node_modules/.bin/electron-builder"
  },
  "author": "Romanov Yuri",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.1",
    "electron": "^8.0.0",
    "electron-builder": "^21.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.22.3",
    "classnames": "^2.2.6",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "build": {
    "appId": "com.poeLadder.app",
    "productName": "PoeLadder",
    "directories": {
      "output": "release"
    },
    "win": {
      "target": [
        "portable"
      ],
      "icon": "electron/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "electron/icon.png"
    }
  }
}
